<div class="container-new-demonstration">
  <h2>Add a demonstration</h2>
    <%= simple_form_for [@demonstration] do |f| %>
    <%= f.input :title %>

    <%= f.input :description %>
    <%= f.input :location %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.input :extra_info%>
    <%= f.input :photo, as: :file %>
    <%= f.button :submit, "Save", class:"btn btn-primary " %>
  <% end %>
</div>
